{
  "name": "qwik-frontend",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite --mode ssr",
    "build": "qwik build",
    "build.client": "vite build",
    "build.ssr": "qwik build ssr",
    "build.ssr:workers": "vite build --config adapters/workers/vite.config.ts",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "preview": "qwik build preview",
    "build.vite.ssr": "vite build --ssr src/entry.ssr.tsx --outDir .qwik/server --emptyOutDir",
    "build.all": "npm run build && npm run build.ssr"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.17.1",
    "@builder.io/qwik-city": "^1.17.1",
    "ignore": "^7.0.5",
    "typescript": "^5.4.5",
    "vite": "^5.4.0",
    "vite-tsconfig-paths": "^5.0.0"
  }
}